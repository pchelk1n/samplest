{
    "name": "bdv/samplest",
    "description": "Learning how to build a PHP framework",
    "license": "MIT",
    "require": {
        "php": "^8.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^13.0",
        "symfony/var-dumper": "^v8.0",
        "friendsofphp/php-cs-fixer": "^3.94",
        "rector/rector": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Samplest\\": "src/",
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    }
}
